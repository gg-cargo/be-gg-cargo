{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug NestJS",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "/Users/fikrihaikal/.nvm/versions/node/v22.20.0/bin/node",
      "env": {
        "PATH": "/Users/fikrihaikal/.nvm/versions/node/v22.20.0/bin:${env:PATH}"
      },
      "runtimeArgs": [
        "--inspect-brk=9229",
        "${workspaceFolder}/node_modules/@nestjs/cli/bin/nest.js",
        "start",
        "--watch"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "sourceMaps": true,
      "restart": true,
      "autoAttachChildProcesses": true,
      "skipFiles": [
        "<node_internals>/**"
      ],
      "envFile": "${workspaceFolder}/.env"
    }
  ]
}